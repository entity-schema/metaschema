{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/schema/array-tuple#",
  "title": "Array Tuple",
  "allOf": [
    {
      "$ref": "http://example.com/schema/array-type#"
    },
    {
      "properties": {
        "items": {
          "type": "array",
          "items": { "$ref": "http://example.com/schema/schema-object#" }
        }
      },
      "required": [ "items" ]
    }
  ]
}
